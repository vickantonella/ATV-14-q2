<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Lista de Usuários</title>
    <link rel="stylesheet" href="/css/lista.css">
</head>
<body>
    <h2>LISTA DE USUÁRIOS</h2>
    <table>
        <thead>
            <tr>
                <th>Usuário:</th>
                <th>Senha:</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(function(user) { %>
                <tr>
                    <td><%= user.username %></td>
                    <td><%= user.password %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
    <a href="/user">Voltar para a página do usuário</a>
</body>
</html>
